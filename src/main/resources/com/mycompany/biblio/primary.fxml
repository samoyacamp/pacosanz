<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" prefHeight="281.0" prefWidth="349.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.biblio.LoginController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <GridPane prefHeight="88.0" prefWidth="262.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Usuario:" />
            <Label prefHeight="17.0" prefWidth="56.0" text="Email:" GridPane.rowIndex="1" />
            <Label prefHeight="17.0" prefWidth="60.0" text="Password:" GridPane.rowIndex="2" />
            <TextField fx:id="nombre" text="Ramon" GridPane.columnIndex="1" />
            <TextField fx:id="pwd" text="123" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="email" text="r@gmail.com" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="login" mnemonicParsing="false" onAction="#login" text="Log In" />
      <Button fx:id="registrer" mnemonicParsing="false" onAction="#registrer" text="Register" />
   </children>
</VBox>
